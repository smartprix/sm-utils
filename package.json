{
  "name": "sm-utils",
  "version": "2.17.23",
  "description": "Uilities for NodeJS",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartprix/sm-utils"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node",
    "test": "npm run lint && npm run build && npm run test-only",
    "check-coverage": "true",
    "build": "rm -rf dist && babel src --out-dir dist",
    "docs": "rm -rf docs && jsdoc -c jsdoc.json",
    "lint": "eslint src test",
    "compile": "tsc",
    "test-only": "NODE_ENV=test mocha --slow 0 --exit --require @babel/register test/*.test.js --icu-data-dir=node_modules/full-icu",
    "-precommit": "npm run build && npm test && npm run check-coverage",
    "prepublishOnly": "npm run build"
  },
  "author": "Hitesh Khandelwal <hitesh@smartprix.com> (http://www.smartprix.com/)",
  "license": "ISC",
  "dependencies": {
    "@smpx/cfg": "^1.2.2",
    "@types/ioredis": "^4.0.10",
    "@types/node": "^12.12.17",
    "@types/tough-cookie": "^2.3.5",
    "chalk": "^3.0.0",
    "chmodr": "^1.2.0",
    "chownr": "^1.1.1",
    "commander": "^4.0.1",
    "etc-passwd": "^0.1.1",
    "glob": "^7.1.3",
    "got": "^9.6.0",
    "ioredis": "^4.9.0",
    "kue": "^0.11.6",
    "lodash": "^4.17.11",
    "micro-observer": "0.0.19",
    "mkdirp": "^0.5.1",
    "rimraf": "^3.0.0",
    "sm-tunnel-agent": "^0.6.4",
    "socks5-client": "^1.2.6",
    "stack-trace": "^0.0.10",
    "timestring": "^6.0.0",
    "tough-cookie": "^3.0.1",
    "tough-cookie-file-store": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "chai": "^4.2.0",
    "docdash": "^1.1.0",
    "eslint": "^5.16.0",
    "eslint-config-smartprix": "^3.5.4",
    "full-icu": "^1.2.1",
    "get-port": "^5.0.0",
    "isparta": "^4.1.1",
    "jsdoc": "^3.5.5",
    "mocha": "^6.0.2",
    "np": "^5.2.1",
    "pem": "^1.14.2",
    "workerpool": "^5.0.2"
  },
  "eslintConfig": {
    "extends": "smartprix",
    "rules": {
      "vue/script-indent": 0
    },
    "env": {
      "node": true,
      "browser": false
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          },
          "modules": "commonjs",
          "shippedProposals": true
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ],
    "sourceMaps": true
  }
}
