{
	"tags": {
		"allowUnknownTags": false
	},
	"source": {
		"include": "src",
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs)"
	},
	"plugins": [
		"plugins/markdown"
	],
	"markdown": {
		"hardwrap": true
	},
	"opts": {
		"template": "node_modules/docdash",
		"encoding": "utf8",
		"destination": "docs/",
		"recurse": true,
		"verbose": true
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"docdash": {
		"search": true,
		"collapse": true
	}
}
